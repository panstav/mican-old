{
  "name": "Mican",
  "description": "Israeli Social Initiatives Index",
  "author": "Stav Pan Geffen <stavgeffen@gmail.com>",
  "main": "index.js",
  "version": "0.0.0",
  "engines": {
    "node": "4.2.6",
    "npm": "2.14.7"
  },
  "homepage": "https://www.mican.co.il",
  "readmeFilename": "README.md",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/panstav/mican.git"
  },
  "bugs": {
    "url": "https://github.com/panstav/mican/issues"
  },
  "config": {
    "pre-git": {
      "pre-commit": "npm run mocha",
      "post-commit": "git st",
      "post-merge": "git recap",
      "pre-push": [
        "npm test",
        "snyk test"
      ],
      "post-push": "git gc",
      "commit-msg": []
    }
  },
  "scripts": {
    "start": "node . | bunyan --color",
    "local": "LOCAL=true DEBUG=true node . | bunyan --color",
    "nodemon": "DEBUG=true gulp nodemon | bunyan --color",
    "debug": "LOCAL=true DEBUG=true SHOW_SRC=true node . | bunyan --color",
    "trace": "LOCAL=true TRACE=true node . | bunyan --color",
    "postinstall": "gulp heroku",
    "log": "heroku logs --tail --app mican",
    "log-dev": "heroku logs --tail --app mican-dev",
    "test": "gulp production-like && npm run mocha && gulp local && npm run mocha",
    "mocha": "LOCAL=true mocha",
    "commit": "commit-wizard"
  },
